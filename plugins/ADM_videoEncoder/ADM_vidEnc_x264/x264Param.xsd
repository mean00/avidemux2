                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value" type="uint8" minOccurs="16" maxOccurs="16"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="intra8x8Luma">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value" type="uint8" minOccurs="64" maxOccurs="64"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="inter8x8Luma">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value" type="uint8" minOccurs="64" maxOccurs="64"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
