                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="reduceFluxAfterCurveCompression">
                      <xs:simpleType>
                        <xs:restriction base="xs:float">
                          <xs:minInclusive value="0"/>
                          <xs:maxInclusive value="1"/>
