                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="intraLumaDeadzone">
                      <xs:simpleType>
                        <xs:restriction base="xs:integer">
                          <xs:minInclusive value="0"/>
                          <xs:maxInclusive value="32"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="rateControl">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="quantiserMinimum">
