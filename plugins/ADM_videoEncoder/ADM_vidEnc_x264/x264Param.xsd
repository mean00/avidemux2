                            <xs:element name="bitrateFactor" type="xs:float"/>
                          </xs:choice>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="accessUnitDelimiters" type="xs:boolean"/>
